{
  "name": "@modules/user-api",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "exports": {
    "./*": "./src/*"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky"
